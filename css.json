  "Font families": {
    "prefix": "?font",
    "body": [
      "--font-ff: \"Ubuntu\", \"Yomogi\", \"Montserrat\", \"Alata\", \"Raleway\", \"Baloo 2\";"
    ],
    "description": "Sets the font families to the fonts that I have"
  },
  "Css reset": {
    "prefix": "?cssreset",
    "body": [
      "* {",
      "\tpadding: 0;",
      "\tmargin: 0;",
      "\tbox-sizing: border-box;",
      "}"
    ],
    "description": "Resets the css file"
  },
  "Flex-Align-Justify Center": {
    "prefix": "?flex",
    "body": [
      "display: flex;",
      "align-items: center;",
      "justify-content: center;"
    ],
    "description": "Adds flexbox with center aligned and justified"
  },
  "Glass background": {
    "prefix": "?glassback",
    "body": [
      "background: linear-gradient(",
      "\tto right top,",
      "\trgba(255, 255, 255, 255, .7),",
      "\trgba(255, 255, 255, 255, .3)",
      ")"
    ],
    "description": "Adds a linear gradient with glass background"
  }
}
